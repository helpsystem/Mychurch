  "type": "module",
  "scripts": {
    "dev": "vite",
    "backend": "cd backend && npm run dev",
    "dev:all": "concurrently --kill-others-on-fail \"npm run backend\" \"npm run dev\"",
    "dev:full": "concurrently --names \"BACKEND,FRONTEND\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\" \"npm run backend\" \"npm run dev\"",
    "build": "vite build",
    "build:analyze": "vite build --mode production && npx vite-bundle-analyzer dist",
    "build:prod": "NODE_ENV=production vite build",
    "build:staging": "NODE_ENV=staging vite build",
    "preview": "vite preview",
    "preview:prod": "vite build && vite preview",
    "start": "serve -s dist -l 5000",
    "start-all": "concurrently \"npm run dev\" \"npm run backend\"",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "echo 'Linting not configured yet'",
    "sitemap:generate": "node scripts/generate-sitemap.js",
    "optimize:images": "node scripts/optimize-images.js"
  },
